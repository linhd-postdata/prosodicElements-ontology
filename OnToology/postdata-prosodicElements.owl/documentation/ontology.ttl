#################################################################
#    Object Properties
#################################################################

###  http://postdata.linhd.uned.es/ontology/postdata-prosodicElements#affectsFirst
:affectsFirst rdf:type owl:ObjectProperty ;
              rdfs:domain :Metaplasm ;
              rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-structuralElements#Word> ;
              rdfs:comment "This property points to the first of the words affected by a metaplasm"@en ;
              rdfs:label "affectsFirst"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-prosodicElements#analyses
:analyses rdf:type owl:ObjectProperty ;
          owl:inverseOf :isAnalysedThrough ;
          rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-core#Redaction> ;
          rdfs:comment "This property points to an instance of Redaction in which a WorkPattern, StanzaPattern or LinePattern presents analytical information about the work as a whole, the stanzas or the lines,  respectively." ;
          rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
          rdfs:label "analyses"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-prosodicElements#clausula
:clausula rdf:type owl:ObjectProperty ;
          rdfs:domain :Foot ;
          rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                       owl:hasValue <http://postdata.linhd.uned.es/kos/clausula>
                     ] ;
          rdfs:comment "The categorisation of the item according to the rhythmic ending of a line  determined by the number of unstressed syllables following the last stressed one. This property is both valid for accentual and syllabic versification, so possible values include: masculine (zero syllables), femenine (one), dactylic, proparoxitone, etc."@en ;
          rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements#clausula"@en ;
          rdfs:label "clausula"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-prosodicElements#clausulaSchemeType
:clausulaSchemeType rdf:type owl:ObjectProperty ;
                    rdfs:domain :StanzaPattern ;
                    rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                 owl:hasValue <http://postdata.linhd.uned.es/kos/clausulaScheme-type>
                               ] ;
                    rdfs:comment "Categorisation of the stanza according to the distribution of clausulae/gender of rhymes (eg. “classic”, “heterogonique”, etc.)"@en ;
                    rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
                    rdfs:label "clausulaSchemeType"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-prosodicElements#corresponds
:corresponds rdf:type owl:ObjectProperty ;
             owl:inverseOf :matches ;
             rdfs:domain :RhymeMatch ;
             rdfs:range :Rhyme ;
             rdfs:comment "This property points to the instance of Rhyme whose rhyming word is described by the current instance of RhymeMatch"@en ;
             rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
             rdfs:label "corresponds"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-prosodicElements#employs
:employs rdf:type owl:ObjectProperty ;
         owl:inverseOf :isEmployedIn ;
         rdfs:domain :MetricalEncoding ;
         rdfs:range :Symbol ;
         rdfs:comment "This propery points to a Symbol used in metrical notation being defined"@en ;
         rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
         rdfs:label "employs"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-prosodicElements#feetType
:feetType rdf:type owl:ObjectProperty ;
          rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                       owl:hasValue <http://postdata.linhd.uned.es/kos/feet-type>
                     ] ;
          rdfs:comment "Categorisation of the stanza or line according to the type of feet"@en ;
          rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
          rdfs:label "feetType"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-prosodicElements#footType
:footType rdf:type owl:ObjectProperty ;
          rdfs:domain :Foot ;
          rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                       owl:hasValue <http://postdata.linhd.uned.es/kos/foot-type>
                     ] ;
          rdfs:comment "Type of foot (e.g. iamb)"@en ;
          rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
          rdfs:label "footType"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-prosodicElements#hasFirstFoot
:hasFirstFoot rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              owl:inverseOf :isFirstFoot ;
              rdfs:domain :LinePattern ;
              rdfs:range :Foot ;
              <http://purl.org/dc/elements/1.1/creator> "mluisadiez" ;
              <http://purl.org/dc/elements/1.1/date> "2018-06-16T20:30:43Z"^^xsd:dateTime ;
              rdfs:comment "This property points to the first Foot of the line" ;
              rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
              rdfs:label "hasFirstFoot"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-prosodicElements#hasFirstUnit
:hasFirstUnit rdf:type owl:ObjectProperty ;
              owl:inverseOf :isFirstUnit ;
              rdfs:domain :Foot ;
              rdfs:range :FootDivision ;
              rdfs:comment "This property points to the firt unit in which the foot is divided (an instance of FootDivision)"@en ;
              rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
              rdfs:label "hasFirstUnit"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-prosodicElements#hasRhymeWord
:hasRhymeWord rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              owl:inverseOf :isRhymeWord ;
              rdfs:domain :Rhyme ;
              rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-structuralElements#Word> ;
              <http://purl.org/dc/elements/1.1/creator> "mluisadiez" ;
              <http://purl.org/dc/elements/1.1/date> "2018-06-16T20:30:43Z"^^xsd:dateTime ;
              rdfs:comment "This property points to the instance of Word for a further linguistic analysis/division by words"@en ;
              rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
              rdfs:label "hasRhymeWord"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-prosodicElements#isAnalysedThrough
:isAnalysedThrough rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdfs:domain <http://postdata.linhd.uned.es/ontology/postdata-core#Redaction> ;
                   rdfs:range :Pattern ;
                   <http://purl.org/dc/elements/1.1/creator> "mluisadiez" ;
                   <http://purl.org/dc/elements/1.1/date> "2018-06-16T20:30:43Z"^^xsd:dateTime ;
                   rdfs:comment "This property points to an item (Pattern) that contains analytical content about the current instance of Redaction or about its stanzas or lines"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-prosodicElements#isEchoOf
:isEchoOf rdf:type owl:ObjectProperty ;
          rdfs:domain <http://postdata.linhd.uned.es/ontology/postdata-structuralElements#Word> ;
          rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-structuralElements#Word> ;
          rdfs:comment "This property points to a following Word (echo) that that rhymes with the current instance of Word"@en ;
          rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
          rdfs:label "isEchoOf"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-prosodicElements#isEmployedIn
:isEmployedIn rdf:type owl:ObjectProperty ;
              rdfs:domain :Symbol ;
              rdfs:range :MetricalEncoding ;
              rdfs:comment "This property points to the instance of MetricalEncoding that employs the item at hand"@en ;
              rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
              rdfs:label "isEmployedIn"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-prosodicElements#isFirstFoot
:isFirstFoot rdf:type owl:ObjectProperty ;
             rdfs:domain :Foot ;
             rdfs:range :LinePattern ;
             rdfs:comment "This property points to the LinePattern whose feet description begins with the current item under analysis"@en ;
             rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
             rdfs:label "isFirstFoot"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-prosodicElements#isFirstUnit
:isFirstUnit rdf:type owl:ObjectProperty ;
             rdfs:domain :FootDivision ;
             rdfs:range :Foot ;
             rdfs:comment "This property points to the Foot that contains the item at hand as its first element"@en ,
                          "isFirstUnit"@en ;
             rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-prosodicElements#isPresentIn
:isPresentIn rdf:type owl:ObjectProperty ;
             owl:inverseOf :presents ;
             rdfs:domain :Rhyme ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( <http://postdata.linhd.uned.es/ontology/postdata-core#Redaction>
                                        <http://postdata.linhd.uned.es/ontology/postdata-structuralElements#Line>
                                        <http://postdata.linhd.uned.es/ontology/postdata-structuralElements#Stanza>
                                      )
                        ] ;
             rdfs:comment "This property points to the text (instance of Redaction, Line or Stanza) which contains the current item under analysis"@en ;
             rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
             rdfs:label "isPresentIn"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-prosodicElements#isRhymeWord
:isRhymeWord rdf:type owl:ObjectProperty ;
             rdfs:domain <http://postdata.linhd.uned.es/ontology/postdata-structuralElements#Word> ;
             rdfs:range :Rhyme ;
             rdfs:comment "This property points to the instance of Rhyme that contains the item at hand as its rhyme word"@en ;
             rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
             rdfs:label "isRhymeWord"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-prosodicElements#isUsedBy
:isUsedBy rdf:type owl:ObjectProperty ;
          owl:inverseOf :uses ;
          rdfs:domain :MetricalEncoding ;
          rdfs:range :Pattern ;
          rdfs:comment "This property points to an instance of LinePattern, StanzaPattern or WorkPattern which uses the convention defined in the current instance of MetricalEncoding"@en ;
          rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
          rdfs:label "isUsedBy"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-prosodicElements#matches
:matches rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:domain :Rhyme ;
         rdfs:range :RhymeMatch ;
         <http://purl.org/dc/elements/1.1/creator> "mluisadiez" ;
         <http://purl.org/dc/elements/1.1/date> "2018-06-16T20:30:43Z"^^xsd:dateTime ;
         rdfs:comment "This property points to the instance of RhymeMatch that contains the echo of the current rhyme word under analysis"@en ;
         rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
         rdfs:label "matches"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-prosodicElements#metricalCategory
:metricalCategory rdf:type owl:ObjectProperty ;
                  rdfs:domain :WorkPattern ;
                  rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                               owl:hasValue <http://postdata.linhd.uned.es/kos/metrical-category>
                             ] ;
                  rdfs:comment "Major category to identify the metrical type (e.g. fixed forms vs strophic)"@en ;
                  rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
                  rdfs:label "metricalCategory"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-prosodicElements#metricalComplexity
:metricalComplexity rdf:type owl:ObjectProperty ;
                    rdfs:domain :WorkPattern ;
                    rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                 owl:hasValue <http://postdata.linhd.uned.es/kos/metrical-complexity>
                               ] ;
                    rdfs:comment "Categorisation depending on the number of syllables: usually, less or more than eight"@en ;
                    rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
                    rdfs:label "metricalComplexity"@en .


